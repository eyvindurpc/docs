  /message/sendMedia/{instance}:
    post:
      tags:
        - Send Message
      summary: Send Media File
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties: {}
      parameters:
        - name: instance
          in: path
          schema:
            type: string
          required: true
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /message/sendPtv/{instance}:
    post:
      tags:
