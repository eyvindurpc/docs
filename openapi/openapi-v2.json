      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /instance/fetchInstances:
    get:
      tags:
        - Instance
      summary: Fetch Instances
      security:
        - apikeyAuth: []
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /instance/connect/{instance}:
    get:
      tags:
        - Instance
      summary: Instance Connect
      parameters:
        - name: instance
          in: path
          schema:
            type: string
          required: true
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /instance/restart/{instance}:
    post:
      tags:
        - Instance
      summary: Restart Instance
      requestBody:
        content: {}
      parameters:
        - name: instance
          in: path
          schema:
            type: string
          required: true
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /instance/setPresence/{instance}:
    post:
