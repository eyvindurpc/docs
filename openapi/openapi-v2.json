  - name: Proxy
  - name: Settings
  - name: Send Message
  - name: Call
  - name: Chat
  - name: Label
  - name: Profile Settings
  - name: Group
  - name: Integrations
  - name: Integrations > Events
  - name: Integrations > Events > Websocket
  - name: Integrations > Events > Rabbitmq
  - name: Integrations > Events > Sqs
