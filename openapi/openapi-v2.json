      summary: Send PTV File
      requestBody:
        content:
          multipart/form-data:
            schema:
              type: object
              properties:
                number:
                  type: string
                  example: '{{remoteJid}}'
                file:
                  type: string
                  format: binary
      parameters:
        - name: instance
          in: path
          schema:
            type: string
          required: true
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
